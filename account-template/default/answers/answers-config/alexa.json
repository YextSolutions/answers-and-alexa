{
  "$schema": "https://schema.yext.com/config/answers/answers-config/v1",
  "$id": "alexa",
  "name": "alexa",
  "supportedLocales": [],
  "countryRestrictions": [],
  "verticals": {
    "faqs": {
      "entityTypes": [
        "faq"
      ],
      "name": "FAQs",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true
        },
        "keywords": {
          "textSearch": true
        },
        "question": {
          "semanticTextSearch": true
        }
      },
      "sortBys": [],
      "source": "KNOWLEDGE_MANAGER"
    },
    "locations": {
      "entityTypes": [
        "location"
      ],
      "name": "Locations",
      "searchableFields": {
        "builtin.entityType": {
          "nlpFilter": true
        },
        "builtin.location": {
          "nlpFilter": true
        },
        "keywords": {
          "phraseMatch": true
        },
        "name": {
          "textSearch": true
        }
      },
      "sortBys": [],
      "source": "KNOWLEDGE_MANAGER"
    }
  },
  "localizations": {},
  "additionalSearchablePlaces": [],
  "querySuggestions": {
    "disablePopularQueries": false,
    "verticalPromptsOnEmpty": false,
    "popularQueriesBlacklistedTerms": [],
    "universalPrompts": [],
    "verticalPrompts": {
      "faqs": [
        "[[name]]"
      ],
      "locations": [
        "Locations near [[address.city]]",
        "Locations near [[address.region]]"
      ]
    }
  }
}